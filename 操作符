#include<stdio.h>
int main()
{
	int a = 3;
	int b = 5;
	int c = a & b;
	//&操作符表示“按（二进制）位与”
	//a:011
	//b:101
	//按位与的运算规则是相同二进制都为“1”结果才为“1”，其余结果为“0”。
	//c:001。
	printf("%d\n",c );
	int d = a | b;
	//| 是按（二进制）位或
	//a:011
	//b:101
	//d:111 
	//按位与的运算原则是相同二进制只要有“1”结果就是“1”，其余结果是“0”
	printf("%d\n", d);
	int e = a ^ b;
	//^是按（二进制）位异或
	//a:011
	//b:101
	//e:110
	//按位异或的运算原则是相同二进制相同就为“0”，相同二进制为不同就为“1”
	printf("%d\n", e);
	return 0;
}
